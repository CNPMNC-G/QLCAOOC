@model IEnumerable<QLCAOOC.Models.CANHO>
@foreach (var canho in Model)
{
    <div class="row">
        <div class="col-lg-4 col-sm-5"><img src="~/images/canho/@canho.HinhAnh" class="img-responsive img-circle" alt="properties" /></div>
        <div class="col-lg-8 col-sm-7">
            <h5><a href="@Url.Action("Details", "Trangchu", new {id = @canho.MaCanHo})">Căn hộ: @canho.MaCanHo</a></h5>
            <p class="price">@string.Format("{0:0,0}", @canho.GIA.Gia1).Replace(",", ".") VNĐ/tháng</p>
        </div>
    </div>
}
